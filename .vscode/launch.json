                "universes",
                "list"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "universes members",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/barbucket.py",
            "args": [
                "universes",
                "members",
                "--name",
                "test_univ"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "universes delete",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/barbucket.py",
            "args": [
                "universes",
                "delete",
                "--name",
                "test_univ"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "quotes fetch",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/barbucket.py",
            "args": [
                "quotes",
                "fetch",
                "--universe",
                "germany_top_800_cap"
            ],
            "console": "integratedTerminal"
        }
    ]
