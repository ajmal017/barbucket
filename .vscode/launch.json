    "version": "0.2.0",
    "configurations": [
        {
            "name": "database reset",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/barbucket.py",
            "args": [
                "database",
                "reset"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "contracts sync_listing",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/barbucket.py",
            "args": [
                "contracts",
                "sync_listing",
                "--contract_type",
                "STOCK",
                "--exchange",
                "NASDAQ"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "tv_details ingest_files",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/barbucket.py",
            "args": [
                "tv_details",
                "ingest_files"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "universes create",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/barbucket.py",
            "args": [
                "universes",
                "create",
                "--universe",
                "test_univ",
                "--contract_ids",
                "[8,18,28,38] " // Trailing space necessary for debugger
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "universes list_universes",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/barbucket.py",
            "args": [
                "universes",
                "list_universes"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "universes members",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/barbucket.py",
            "args": [
                "universes",
                "members",
                "--name",
                "test_univ"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "universes delete",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/barbucket.py",
            "args": [
                "universes",
                "delete",
                "--name",
                "test_univ"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "quotes fetch",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/barbucket.py",
            "args": [
                "quotes",
                "fetch",
                "--universe",
                "test_univ"
            ],
            "console": "integratedTerminal"
        }
    ]
}
