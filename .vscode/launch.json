    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "database reset",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/barbucket.py",
            "args": [
                "database",
                "reset"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "contracts sync-listing",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/barbucket.py",
            "args": [
                "contracts",
                "sync-listing",
                "--type",
                "STOCK",
                "--exchange",
                "NYSE"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "contracts fetch-ib-details",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/barbucket.py",
            "args": [
                "contracts",
                "fetch-ib-details"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "contracts fetch-tv-details",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/barbucket.py",
            "args": [
                "contracts",
                "fetch-tv-details"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "universes create",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/barbucket.py",
            "args": [
                "universes",
                "create",
                "--name",
                "test_univ",
                "--contract_ids",
                "1,2,3"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "universes list",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/barbucket.py",
